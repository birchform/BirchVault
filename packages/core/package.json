{
  "name": "@birchvault/core",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./crypto": {
      "types": "./dist/crypto/index.d.ts",
      "import": "./dist/crypto/index.mjs",
      "require": "./dist/crypto/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    },
    "./totp": {
      "types": "./dist/totp/index.d.ts",
      "import": "./dist/totp/index.mjs",
      "require": "./dist/totp/index.js"
    },
    "./webauthn": {
      "types": "./dist/webauthn/index.d.ts",
      "import": "./dist/webauthn/index.mjs",
      "require": "./dist/webauthn/index.js"
    },
    "./sharing": {
      "types": "./dist/sharing/index.d.ts",
      "import": "./dist/sharing/index.mjs",
      "require": "./dist/sharing/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@birchvault/config": "workspace:*",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}

